  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.13.2",
    "express-validator": "^7.0.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
