{
  "name": "gaming-cafe-backend",
  "version": "1.0.0",
  "description": "Backend API for Gaming Cafe discovery and booking platform on Firebase Functions",
  "main": "index.js",
  "scripts": {
    "serve": "firebase emulators:start --only functions",
